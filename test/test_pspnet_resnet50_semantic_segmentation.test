<launch>
  <include file="$(find ros_chainercv)/sample/sample_pspnet_semantic_segmentation.launch">
    <arg name="gui" value="false" />
    <arg name="model" value="pspnet_resnet50" />
    <arg name="gpu" value="-1" />
  </include>

  <test test-name="test_pspnet_resnet50_semantic_segmentation"
        pkg="jsk_tools" type="test_topic_published.py"
        retry="3" time-limit="360">
    <rosparam>
      topic_0: /semantic_segmentation/output/vis
      timeout_0: 240
      topic_2: /semantic_segmentation/output/label
      timeout_2: 240
    </rosparam>
  </test>

</launch>
