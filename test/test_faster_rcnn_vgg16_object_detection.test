<launch>
  <include file="$(find ros_chainercv)/sample/sample_faster_rcnn_object_detection.launch">
    <arg name="gui" value="false" />
    <arg name="model" value="faster_rcnn_vgg16" />
    <arg name="gpu" value="-1" />
  </include>

  <test test-name="test_faster_rcnn_vgg16_object_detection"
        pkg="jsk_tools" type="test_topic_published.py"
        retry="3" time-limit="360">
    <rosparam>
      topic_0: /object_detection/output/vis
      timeout_0: 360
      topic_1: /object_detection/output/class
      timeout_1: 360
      topic_2: /object_detection/output/rects
      timeout_2: 360
    </rosparam>
  </test>

</launch>
